{
  "version": 2,
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "^/socket.io/(.*)",
      "dest": "/socket.io/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html"
    }
  ]
}
